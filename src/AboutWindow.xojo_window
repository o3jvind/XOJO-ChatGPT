#tag Events OKButton
	#tag Event
		Sub Pressed()
		  Preferences.ApiKey = ApiKeyTextField.Text
		  
		  If Not Preferences.Save Then
		    MsgBox("Could not save preferences.")
		  End If
		  
		  Self.Close
		End Sub
	#tag EndEvent
